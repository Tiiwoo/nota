{
  "name": "@nota-lang/nota-editor",
  "version": "0.2.19",
  "description": "Browser-based editor for Nota documents",
  "author": "Will Crichton <wcrichto@cs.stanford.edu>",
  "homepage": "https://github.com/nota-lang/nota#readme",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nota-lang/nota.git"
  },
  "scripts": {
    "build": "node build.mjs",
    "tc": "tsc -emitDeclarationOnly",
    "full": "yarn tc && yarn build",
    "lint": "eslint --ext js,ts,tsx lib",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/nota-lang/nota/issues"
  },
  "dependencies": {
    "@babel/standalone": "^7.16.4",
    "@nota-lang/nota-components": "^0.2.19",
    "@nota-lang/nota-syntax": "^0.2.19",
    "exenv": "^1.2.2",
    "prettier": "^2.5.1"
  },
  "dependenciesComments": [
    "We do NOT need to include codemirror, react, etc as peerDependencies b/c they are automatically fetched from @nota-lang/nota-components/dist/"
  ],
  "devDependencies": {
    "@nota-lang/esbuild-utils": "^0.2.19",
    "@nota-lang/nota-common": "^0.2.19",
    "@types/exenv": "^1.2.0",
    "@types/prettier": "^2.4.2",
    "react-error-boundary": "^3.1.4"
  }
}
